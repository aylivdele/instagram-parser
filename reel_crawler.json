{
  "id": "4aad3ef0de6a5cea65b48dd575531a8c",
  "object": "crawler",
  "account": null,
  "name": "Instagram Reels Scraper",
  "slug": "instagram-reels-scraper",
  "description": "Collect reels by URL or from a profile",
  "rank": null,
  "icon": "",
  "is_public": true,
  "is_available": true,
  "is_premium": false,
  "has_issues": false,
  "input": [
    {
      "name": "url",
      "type": "string",
      "level": "task",
      "regex": "^https://(www\\.)?instagram\\.com/(reel/[A-Za-z0-9_-]+|p/[A-Za-z0-9_-]+|[A-Za-z0-9._]+)/?$",
      "default": "",
      "example": "https://www.instagram.com/leomessi/",
      "required": true,
      "description": "Instagram Reel URL or Profile URL. If a profile URL is given, the scraper will collect reels from that profile."
    },
    {
      "name": "max_unique_results_per_run",
      "type": "int",
      "level": "squid",
      "display": "Max Unique Results",
      "required": false,
      "is_details": true,
      "description": "Maximum number of unique results retrieved across all tasks in the entire run. Leave empty for unlimited."
    },
    {
      "name": "max_results",
      "type": "integer",
      "level": "squid",
      "default": null,
      "display": "Max Results Per Task",
      "example": 50,
      "required": false,
      "is_details": true,
      "description": "Total reels the crawler will collect during the run."
    },
    {
      "name": "hours_back",
      "type": "int",
      "level": "squid",
      "default": null,
      "example": 24,
      "required": false,
      "attribute": "timestamp",
      "is_details": true,
      "description": "If `hours_back` is set at 24, the squid will collect reels from the last 24h!"
    },
    {
      "name": "skip_pinned_reels",
      "type": "boolean",
      "level": "squid",
      "default": false,
      "required": false,
      "is_details": false,
      "description": "If enabled, pinned reels will be excluded from the results."
    }
  ],
  "result": [
    "reel_id",
    "shortcode",
    "reel_url",
    "owner_username",
    "owner_full_name",
    "owner_id",
    "likes_count",
    "comments_count",
    "views_count",
    "hashtags",
    "mentions",
    "tagged_users",
    "product_type",
    "timestamp",
    "caption",
    "video_url",
    "display_url",
    "video_duration_seconds",
    "music_info",
    "location_id",
    "location_name",
    "is_sponsored",
    "is_pinned",
    "comments_disabled",
    "like_and_view_counts_disabled",
    "can_viewer_reshare",
    "co_authors",
    "media_dimensions",
    "functions",
    "comment1",
    "comment2",
    "comment3",
    "comment4",
    "comment5",
    "comment6",
    "comment7",
    "comment8",
    "comment9",
    "comment10"
  ],
  "max_concurrency": 20,
  "credits_per_row": { "legacy": 1, "current": 1 },
  "credits_per_email": { "legacy": 2, "current": 2 },
  "default_worker_stats": null,
  "email_worker_stats": null,
  "has_email_verification": false
}
